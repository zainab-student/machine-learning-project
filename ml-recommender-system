   "metadata": {},
   "outputs": [],
   "source": [
    "import streamlit as st\n",
    "import pickle\n",
    "import pandas as pd\n",
    "import os\n",
    "\n",
    "# Define correct path\n",
    "file_path = os.path.join('movie-recommandation-system', 'movie_dict.pkl')\n",
    "\n",
    "# Load the pickle file properly\n",
    "try:\n",
    "    with open(file_path, 'rb') as f:\n",
    "        movie_dict = pickle.load(f)\n",
    "\n",
    "    # Convert to DataFrame if needed\n",
    "    movies = pd.DataFrame.from_dict(movie_dict)\n",
    "\n",
    "    st.success(\"✅ movie_dict.pkl loaded successfully!\")\n",
    "    st.dataframe(movies.head())\n",
    "\n",
    "except FileNotFoundError:\n",
    "    st.error(f\"❌ File not found: {file_path}\")\n",
    "except pickle.UnpicklingError:\n",
    "    st.error(\"❌ Failed to load pickle file. It may be corrupted.\")\n",
    "except Exception as e:\n",
    "    st.error(f\"❌ Unexpected error: {e}\")\n"
   ]
  },
