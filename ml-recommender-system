    "import streamlit as st\n",
    "import pickle\n",
    "import pandas as pd\n",
    "\n",
    "# Use the correct path to your pickle file\n",
    "file_path = 'movie-recommandation-system/movie_dict.pkl'\n",
    "\n",
    "try:\n",
    "    with open(file_path, 'rb') as f:\n",
    "        movie_dict = pickle.load(f)\n",
    "    \n",
    "    # Optional: Convert to DataFrame if it's a dictionary of lists\n",
    "    movies = pd.DataFrame.from_dict(movie_dict)\n",
    "    st.success(\"✅ movie_dict.pkl loaded successfully!\")\n",
    "\n",
    "    # Preview the DataFrame\n",
    "    st.dataframe(movies.head())\n",
    "\n",
    "except FileNotFoundError:\n",
    "    st.error(f\"❌ File not found at: {file_path}\")\n",
    "except Exception as e:\n",
    "    st.error(f\"❌ Error loading pickle file: {e}\")\n"
   ]
