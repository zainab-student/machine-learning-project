    "# Ensure each element is a list of strings before joining\n",
    "def safe_join(x):\n",
    "    if isinstance(x, list):\n",
    "        return \" \".join(map(str, x))  # Convert all elements to strings and join\n",
    "    else:\n",
    "        return \"\"  # or handle as needed (e.g., str(x))\n",
